{
  "version": 2,
  "builds": [
    {
      "src": "frontend/my-portfolio/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/my-portfolio/build/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/api/$1"
    }
  ]
}
